<!DOCTYPE html>
<html>
<head>
  <title>FunWithGeocoder</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
  <script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyC5OJQyqH4vVdhtl5XdwcUqQUuReSyuKLU&callback=initMap" async defer></script>
</head>
<body>
	<nav class="navbar navbar-light bg-faded">
	  <button class="navbar-toggler hidden-sm-up" type="button" data-toggle="collapse" data-target="#exCollapsingNavbar2">
	    &#9776;
	  </button>
	  <div class="collapse navbar-toggleable-xs" id="exCollapsingNavbar2">
	  	<%= link_to("GeocoderFun", root_path, class: 'navbar-brand') %>
	    <ul class="nav navbar-nav">
	      <li class="nav-item <%= 'active' if current_page?(new_place_path) %>">
	      	<%= link_to('Add Place', new_place_path, class: "nav-link") %>
	      </li>
	      <li class="nav-item <%= 'active' if current_page?(new_distance_path) %>">
	        <%= link_to('Calculate Distances', new_distance_path, class: "nav-link") %>
	      </li>
	    </ul>
	  </div>
	</nav>
<br><br>


  

	<div class="container">
		<% flash.each do |name, msg| %>
			<%= content_tag(:div, msg.html_safe, class: "alert alert-#{name}") %>
		<% end %>
		<%= yield %>
	</div>
</body>
</html>
